{"version":"0.3.0","body":"function main(workbook: ExcelScript.Workbook) {\n    // Set your sheet names and column here\n    const sourceSheetName = \"drivers\";\n    const chevroletSheetName = \"Chevrolets\";\n    const columnToCheck = 4; // Column D (1=A, 2=B, 3=C, 4=D)\n\n    // Get the worksheets\n    const sourceSheet = workbook.getWorksheet(sourceSheetName);\n    let chevroletSheet = workbook.getWorksheet(chevroletSheetName);\n\n    // Create \"Chevrolets\" sheet if it doesn't exist\n    if (!chevroletSheet) {\n        chevroletSheet = workbook.addWorksheet(chevroletSheetName);\n    }\n\n    // Get the used range from source sheet\n    const usedRange = sourceSheet.getUsedRange();\n    const values = usedRange.getValues();\n\n    // Get header row\n    const headers = values[0];\n\n    // Array to hold Chevrolet rows\n    const chevroletRows: (string | number | boolean)[][] = [];\n\n    // Iterate through rows (starting at 1 to skip header)\n    for (let i = 1; i < values.length; i++) {\n        const cellValue = String(values[i][columnToCheck - 1]); // -1 because arrays are 0-indexed\n\n        if (cellValue === \"Chevrolet\") {\n            chevroletRows.push(values[i]);\n        }\n    }\n\n    // Set up \"Chevrolets\" sheet with headers if it's empty\n    if (chevroletSheet.getUsedRange() === undefined || chevroletSheet.getUsedRange().getRowCount() === 0) {\n        const headerRange = chevroletSheet.getRangeByIndexes(0, 0, 1, headers.length);\n        headerRange.setValues([headers]);\n    }\n\n    // Add Chevrolet rows to \"Chevrolets\" sheet\n    if (chevroletRows.length > 0) {\n        const chevroletLastRow = chevroletSheet.getUsedRange()?.getRowCount() || 0;\n        const chevroletTarget = chevroletSheet.getRangeByIndexes(chevroletLastRow, 0, chevroletRows.length, values[0].length);\n        chevroletTarget.setValues(chevroletRows);\n    }\n}","description":"","parameterInfo":"{\"version\":1,\"originalParameterOrder\":[],\"parameterSchema\":{\"type\":\"object\",\"default\":{},\"x-ms-visibility\":\"internal\"},\"returnSchema\":{\"type\":\"object\",\"properties\":{}},\"signature\":{\"comment\":\"\",\"parameters\":[{\"name\":\"workbook\",\"comment\":\"\"}]}}","apiInfo":"{\"variant\":\"synchronous\",\"variantVersion\":2}"}